{
  "name": "Open Terminal",
  "version": "1.0.1",
  "description": "Open terminal here.",
  "blocks": {
    "input": [
      {
        "id": "OpenTerminalKey",
        "type": "Keyword",
        "keyword": "term",
        "title": "Open Terminal Here",
        "icon": "fa-terminal",
        "connections": [
          "OpenTerminal"
        ]
      },
      {
        "id": "OpenTerminalInDirectoryKey",
        "type": "PrefixScript",
        "keyword": "term",
        "space": true,
        "args": "Required",
        "script": "open_terminal_in_directory.js",
        "debounce": 75,
        "connections": [
          "OpenTerminalInDirectory"
        ]
      }
    ],
    "output": [
      {
        "id": "OpenTerminal",
        "type": "UserScript",
        "script": "open_terminal.js"
      },
      {
        "id": "OpenTerminalInDirectory",
        "type": "UserScript",
        "script": "open_terminal_in_directory.js"
      }
    ]
  }
}
